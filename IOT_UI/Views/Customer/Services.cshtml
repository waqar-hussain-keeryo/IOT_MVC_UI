<!-- Include Partial View -->
@Html.Partial("_navigation")

<main role="main" id="main" class="main">
    <!-- Page Title -->
    <div class="pagetitle">
        <h1>Digital Service</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Digital Service</a></li>
                <li class="breadcrumb-item active">Add Digital Service</li>
            </ol>
        </nav>
        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#addService">
            Add Digital Service
        </button>
    </div>

    <!-- Page Section -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <!-- Table with stripped rows -->
                        <table class="table table-bordered" id="tbl_Service"></table>
                    </div>
                </div>

            </div>
        </div>
    </section>

    <!-- Add Service Partial View -->
    @Html.Partial("_AddService")

    <!-- Update Service Partial View -->
    @Html.Partial("_UpdateService")

    <script src="~/connectivity/Service.js"></script>
</main>
